<p id="notice"><%= notice %></p>

<h1>Produtos</h1>

<table>
  <thead>
    <tr>
      <th>Preco</th>
      <th>Loja</th>
      <th>Estado</th>
      <th>Quantidade</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @produtos.each do |produto| %>
      <tr>
        <td><%= produto.preco %></td>
        <td><%= produto.loja %></td>
        <td><%= produto.estado %></td>
        <td><%= produto.quantidade %></td>
        <td><%= link_to 'Show', produto %></td>
      </tr>
    <% end %>
  </tbody>
</table>
